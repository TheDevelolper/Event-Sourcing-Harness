{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    
    "toggles": {
        "UseRabbitMq": true,
        "UseEventStore": false
    },
    
    "RabbitMQ": {
        "Host": "rabbitmq",
        "Port": 5672
    },
    
    "EventStore": {
        "ConnectionString": "esdb://eventstore:2113?tls=false"
    }
}